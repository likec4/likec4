{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "dependsOn": [
        "topo"
      ],
      "inputs": [
        "generate.ts",
        "package.json"
      ],
      "outputs": [
        "src/generated.ts"
      ]
    },
    "build": {
      "dependsOn": [
        "generate",
        "^build"
      ],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
