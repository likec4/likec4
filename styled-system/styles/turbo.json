{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "dependsOn": [
        "@likec4/style-preset#build"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "generate",
        "@likec4/style-preset#typecheck"
      ],
      "outputs": [
        "lib/**",
        "*.tsbuildinfo"
      ],
      "outputLogs": "new-only"
    }
  }
}
