{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "dependsOn": [
        "@likec4/style-preset#generate",
        "^topo"
      ],
      "inputs": [
        "dev.ts",
        "preset.ts",
        "panda.config.ts"
      ],
      "outputs": [
        "dist/**"
      ]
    },
    "build": {
      "dependsOn": [
        "@likec4/style-preset#build",
        "generate"
      ],
      "outputs": [
        "dist/**",
        "preset.d.mts",
        "preset.mjs"
      ]
    }
  }
}
