{
  "name": "@likec4/styles",
  "license": "MIT",
  "version": "1.26.1",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "type": "module",
  "sideEffects": false,
  "files": [
    "dist",
    "styled-system",
    "postcss.d.mts",
    "postcss.mjs",
    "dev.ts",
    "preset.d.mts",
    "preset.mjs",
    "!**/*.spec.*",
    "!**/*.d.ts.map"
  ],
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --build --verbose",
    "build": "unbuild",
    "generate": "wireit",
    "emit-pkg": "wireit"
  },
  "wireit": {
    "generate": {
      "command": "panda codegen && panda cssgen"
    },
    "emit-pkg": {
      "command": "panda emit-pkg --base dist",
      "dependencies": [
        "generate"
      ]
    }
  },
  "exports": {
    "./css": {
      "types": "./dist/css/index.d.ts",
      "default": "./dist/css/index.mjs"
    },
    "./tokens": {
      "types": "./dist/tokens/index.d.ts",
      "default": "./dist/tokens/index.mjs"
    },
    "./types": {
      "types": "./dist/types/index.d.ts"
    },
    "./patterns": {
      "types": "./dist/patterns/index.d.ts",
      "default": "./dist/patterns/index.mjs"
    },
    "./jsx": {
      "types": "./dist/jsx/index.d.ts",
      "default": "./dist/jsx/index.mjs"
    },
    "./postcss": {
      "types": "./postcss.d.mts",
      "default": "./postcss.mjs"
    },
    "./recipes": {
      "types": "./dist/recipes/index.d.ts",
      "default": "./dist/recipes/index.mjs"
    },
    "./preset": {
      "sources": "./preset.ts",
      "default": {
        "types": "./dist/preset.d.mts",
        "import": "./dist/preset.mjs",
        "default": "./dist/preset.mjs"
      }
    },
    "./dev": "./dev.ts"
  },
  "dependencies": {
    "@pandacss/dev": "catalog:css"
  },
  "devDependencies": {
    "@pandabox/utils": "^0.0.5",
    "@likec4/style-preset": "workspace:*",
    "@likec4/tsconfig": "workspace:*",
    "@pandabox/panda-plugins": "^0.0.8",
    "std-env": "catalog:utils",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "postcss": "catalog:css",
    "wireit": "catalog:",
    "remeda": "catalog:utils",
    "unbuild": "catalog:",
    "typescript": "catalog:"
  },
  "packageManager": "pnpm@10.6.5"
}
