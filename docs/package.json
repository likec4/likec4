{
  "name": "@likec4/docs-website",
  "version": "1.2.2",
  "private": true,
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "packageManager": "yarn@4.3.0",
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git"
  },
  "scripts": {
    "typecheck:turbo": "run -T turbo run typecheck --filter='docs-website'",
    "typecheck": "tsc --noEmit",
    "build:turbo": "run -T turbo run build --filter='docs-website'",
    "generate:indexpage": "likec4 gen react-legacy -o components/index-page/generated.tsx ./likec4/index-page",
    "generate:bigbank": "likec4 gen react-legacy -o components/example-big-bank/generated.tsx ./likec4/bigbank",
    "generate": "run generate:bigbank && run generate:indexpage",
    "dev": "run generate && next dev",
    "build": "next build",
    "start": "next start",
    "clean": "run -T rimraf -v .next out .turbo",
    "shadcn-ui": "yarn dlx shadcn-ui -- "
  },
  "dependencies": {
    "@likec4/core": "workspace:*",
    "@likec4/diagrams": "workspace:*",
    "@likec4/generators": "workspace:*",
    "@likec4/language-server": "workspace:*",
    "@likec4/layouts": "workspace:*",
    "@monaco-editor/react": "^4.5.0",
    "@radix-ui/react-dropdown-menu": "^2.0.6",
    "@radix-ui/react-scroll-area": "^1.0.5",
    "@react-hookz/web": "^24.0.4",
    "body-scroll-lock-upgrade": "^1.1.0",
    "class-variance-authority": "^0.7.0",
    "clsx": "^2.1.1",
    "jotai": "^2.6.4",
    "langium": "^3.0.0",
    "lucide-react": "^0.294.0",
    "monaco-editor": "0.36.1",
    "monaco-languageclient": "5.0.1",
    "next": "13.4.19",
    "nextra": "^2.13.2",
    "nextra-theme-docs": "^2.13.2",
    "rambdax": "^9.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-icons": "^4.12.0",
    "swr": "^2.2.5",
    "tailwind-merge": "^1.14.0",
    "tailwindcss-animate": "^1.0.7",
    "vscode": "npm:@codingame/monaco-vscode-api@1.76.6",
    "vscode-jsonrpc": "8.2.0",
    "vscode-languageclient": "9.0.1",
    "vscode-languageserver": "9.0.1",
    "vscode-languageserver-protocol": "3.17.5",
    "vscode-oniguruma": "1.7.0",
    "vscode-textmate": "^9.0.0",
    "vscode-uri": "3.0.8"
  },
  "devDependencies": {
    "@types/node": "^20.13.0",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "@types/vscode": "~1.88.0",
    "autoprefixer": "^10.4.19",
    "likec4": "workspace:*",
    "postcss": "^8.4.38",
    "remark-code-import": "^1.2.0",
    "tailwindcss": "^3.4.1",
    "typescript": "^5.4.5",
    "typescript-plugin-css-modules": "^5.1.0"
  }
}
