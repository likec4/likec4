{
  "name": "@likec4/docs-website",
  "version": "0.7.0",
  "private": true,
  "license": "UNLICENSED",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://docs.like-c4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "packageManager": "yarn@3.4.1",
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git"
  },
  "scripts": {
    "compile": "tsc --noEmit",
    "turbo-build": "run -T turbo run build --filter='docs-website...'",
    "generate": "likec4 generate --target react --output components/likec4/generated.tsx ./likec4",
    "dev": "run generate && next dev",
    "build": "next build",
    "start": "next start",
    "clean": "run -T rimraf .next out"
  },
  "dependencies": {
    "@likec4/diagrams": "workspace:*",
    "next": "^13.3.0",
    "nextra": "^2.4.2",
    "nextra-theme-docs": "^2.4.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@likec4/cli": "workspace:*",
    "@types/node": "^18.15.11",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "remark-code-import": "^1.1.1",
    "typescript": "^5.0.4"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
