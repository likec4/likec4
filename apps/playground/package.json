{
  "name": "@likec4/playground",
  "version": "1.41.0",
  "private": true,
  "scripts": {
    "generate": "wireit",
    "dev": "wireit",
    "typecheck": "tsc --build --verbose",
    "build": "vite build",
    "preview": "vite preview",
    "clean": "pnpm rimraf dist lib node_modules/.vite",
    "wrangler:deploy": "wrangler deploy -c dist/playground/wrangler.json",
    "wrangler:preview": "wrangler versions upload -c dist/playground/wrangler.json",
    "deploy:prod": "wireit",
    "deploy:preview": "wireit"
  },
  "wireit": {
    "generate-tsr": {
      "command": "tsr generate",
      "files": [
        "src/routes/**/*.tsx"
      ],
      "output": [
        "src/routeTree.gen.ts"
      ]
    },
    "generate-wrangler": {
      "command": "wrangler types",
      "files": [
        "wrangler.toml",
        "package.json"
      ],
      "output": [
        "worker-configuration.d.ts"
      ]
    },
    "generate": {
      "dependencies": [
        "generate-tsr",
        "generate-wrangler"
      ]
    },
    "dev": {
      "dependencies": [
        "generate"
      ],
      "command": "vite dev"
    },
    "build-with-turbo": {
      "command": "turbo run build"
    },
    "build-and-preview": {
      "dependencies": [
        "build-with-turbo",
        "wrangler:preview"
      ]
    },
    "deploy:prod": {
      "dependencies": [
        "build-with-turbo"
      ],
      "command": "wrangler deploy -c dist/playground/wrangler.json",
      "env": {
        "NODE_ENV": "production",
        "CLOUDFLARE_ENV": "production"
      }
    },
    "deploy:preview": {
      "dependencies": [
        "build-with-turbo"
      ],
      "command": "wrangler versions upload -c dist/playground/wrangler.json",
      "env": {
        "NODE_ENV": "production",
        "CLOUDFLARE_ENV": "production"
      }
    }
  },
  "imports": {
    "#worker": "./worker/index.ts",
    "#worker/client": "./worker/client.ts",
    "#worker/types": "./worker/types.ts"
  },
  "sideEffects": false,
  "type": "module",
  "dependencies": {
    "@cloudflare/vite-plugin": "catalog:cloudflare",
    "@codingame/monaco-vscode-editor-api": "16.1.1",
    "@codingame/monaco-vscode-editor-service-override": "16.1.1",
