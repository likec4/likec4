{
  "name": "@likec4/playground",
  "type": "module",
  "version": "1.4.0",
  "private": true,
  "scripts": {
    "generate": "tsr generate",
    "dev": "vite dev --clearScreen false",
    "typecheck": "tsc --noEmit",
    "build:turbo": "run -T turbo run build --filter='@likec4/playground' --log-prefix=none --log-order=grouped",
    "build": "vite build --mode pre && vite build",
    "deploy": "npx wrangler pages deploy dist --project-name likec4-playground",
    "deploy:prod": "run deploy --branch main",
    "clean": "run -T rimraf -v dist"
  },
