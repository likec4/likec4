{
  "name": "@likec4/playground",
  "version": "1.25.0",
  "private": true,
  "scripts": {
    "generate": "wireit",
    "dev": "wireit",
    "typecheck": "tsc --noEmit",
    "build": "vite build",
    "preview": "vite preview",
    "wrangler:deploy": "wireit",
    "wrangler:preview": "wireit",
    "deploy:prod": "wireit",
    "clean": "rm -r -f dist"
  },
  "wireit": {
    "generate-panda": {
      "command": "panda codegen",
      "files": [
        "package.json",
        "src/index.css",
        "panda.config.ts"
      ],
      "output": [
        "styled-system/*"
      ]
    },
    "generate-tsr": {
      "command": "tsr generate",
      "files": [
        "package.json",
        "tsr.config.json",
        "vite.config.ts",
        "src/routes/*.tsx"
      ],
      "output": [
        "src/routeTree.gen.ts"
      ]
    },
    "generate-wrangler": {
      "command": "wrangler types --experimental-include-runtime",
      "files": [
        "package.json",
        "tsconfig.worker.json",
        "wrangler.toml"
      ],
      "output": [
        ".wrangler/types",
        "worker-configuration.d.ts"
      ]
    },
    "generate": {
      "dependencies": [
        "generate-panda",
        "generate-tsr",
        "generate-wrangler"
      ]
    },
    "dev": {
      "command": "vite dev",
      "dependencies": [
        "generate",
        "../../packages/diagram:build:watch"
      ]
    },
    "build-with-turbo": {
      "command": "turbo run build"
    },
    "build-and-preview": {
      "dependencies": [
        "build-with-turbo",
        "wrangler:preview"
      ]
    },
    "deploy:prod": {
      "dependencies": [
        "build-with-turbo"
      ],
      "command": "wrangler deploy -c dist/playground/wrangler.json",
      "env": {
        "NODE_ENV": "production",
        "CLOUDFLARE_ENV": "production"
      }
    },
    "wrangler:deploy": {
      "command": "wrangler deploy -c dist/playground/wrangler.json"
    },
    "wrangler:preview": {
      "command": "wrangler versions upload -c dist/playground/wrangler.json"
    }
  },
  "imports": {
    "#worker": "./worker/index.ts",
    "#worker/types": "./worker/types.ts"
  },
  "sideEffects": false,
  "type": "module",
  "packageManager": "pnpm@10.5.2",
  "dependencies": {
    "type-fest": "4.34.1"
  },
  "devDependencies": {
