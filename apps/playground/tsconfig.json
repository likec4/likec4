{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@likec4/tsconfig/react-lib.json",
  "compilerOptions": {
    "jsx": "react-jsx",
    "types": [
      "node",
      "vite/client"
    ],
    "noEmit": true,
    "paths": {
      "#monaco/*": [
        "./src/monaco/*"
      ],
    },
    "customConditions": [
      "sources"
    ],
    "noUnusedParameters": false
  },
  "include": [
    "./src",
  ]
}
