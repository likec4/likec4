{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "globalEnv": [
    "NODE_ENV",
    "CLOUDFLARE_ENV"
  ],
  "tasks": {
    "generate": {
      "outputs": [
        "src/routeTree.gen.ts",
        ".wrangler/types",
        "styled-system",
        "worker-configuration.d.ts"
      ],
      "inputs": [
        "*.config.ts",
        "wrangler.toml",
        "src/routes/*.tsx",
        "tsr.config.json",
        "package.json"
      ]
    },
    "build": {
      "dependsOn": [
        "generate",
        "^build"
      ],
      "inputs": [
        "$TURBO_DEFAULT$",
        "!src/routeTree.gen.ts",
        "!tsr.config.json"
      ],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
