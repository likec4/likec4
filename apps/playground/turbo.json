{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "outputs": [
        "src/routeTree.gen.ts",
        "styled-system",
        "worker-configuration.d.ts"
      ]
    },
    "build": {
      "env": [
        "NODE_ENV",
        "CLOUDFLARE_ENV"
      ],
      "dependsOn": [
        "^generate",
        "generate",
        "topo",
        "@likec4/core#build"
      ],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
