{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "outputs": [
        "src/routeTree.gen.ts",
        "styled-system",
        "worker-configuration.d.ts"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "@likec4/diagram#typecheck",
        "@likec4/language-server#typecheck",
        "generate",
        "topo"
      ],
      "outputs": [
        "lib/**"
      ],
      "outputLogs": "new-only"
    },
    "build": {
      "env": [
        "NODE_ENV",
        "CLOUDFLARE_ENV"
      ],
      "dependsOn": [
        "generate",
        "@likec4/language-server#build"
      ],
      "outputs": [
        "dist/**"
      ]
    }
  }
}
