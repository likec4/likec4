{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "dependsOn": [
        "@likec4/styles#generate",
        "@likec4/language-server#generate"
      ],
      "outputs": [
        "src/routeTree.gen.ts",
        "styled-system/**",
        "worker-configuration.d.ts"
      ]
    },
    "build": {
      "dependsOn": [
        "generate",
        "@likec4/language-server#build"
      ],
      "outputs": [
        ".wrangler/deploy/**",
        "dist/**"
      ]
    }
  }
}
