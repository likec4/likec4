{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "moduleResolution": "Bundler",
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@assets/*": [
        "src/assets/*"
      ]
    }
  }
}
