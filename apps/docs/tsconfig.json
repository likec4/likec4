{
  "extends": "astro/tsconfigs/strictest",
  "compilerOptions": {
    "noUnusedLocals": false,
    "noUnusedParameters": false,
    "moduleResolution": "Bundler",
    "checkJs": false,
    "skipLibCheck": true,
    "noEmit": true,
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "./src/components/*"
      ],
      "@assets/*": [
        "./src/assets/*"
      ]
    },
    "jsx": "react-jsx",
    "jsxImportSource": "react"
  },
  "include": [
    "./src"
  ],
  "exclude": [
    "node_modules",
    "**/node_modules/**",
    "dist"
  ],
  "references": [
    {
      "path": "../../packages/likec4/tsconfig.json"
    },
  ]
}
