{
  "name": "@likec4/docs-astro",
  "type": "module",
  "version": "1.2.1",
  "packageManager": "yarn@4.3.0",
  "private": true,
  "scripts": {
    "gupdate-theme-c4": "tsx scripts/generate-theme-c4.mts",
    "generate:dynamic": "likec4 gen react -o src/components/dynamic-view/likec4-views.jsx ./likec4/dynamic-view",
    "generate:theme": "likec4 gen react -o src/components/likec4-theme/likec4-views.jsx ./likec4/theme",
    "generate:bigbank": "likec4 gen react -o src/components/bigbank/likec4-views.jsx ./likec4/bigbank",
    "generate:index": "likec4 gen react -o src/components/index-page/likec4-views.jsx ./likec4/index-page",
    "generate": "run-p 'generate:*'",
    "build:turbo": "run -T turbo run build --filter='@likec4/docs-astro'",
    "dev": "astro dev",
    "start": "astro dev",
    "check": "astro check",
    "typecheck": "tsc --noEmit",
    "build": "run generate && astro check && astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "npx wrangler pages deploy dist ----project-name likec4-docs-astro",
    "deploy:prod": "run deploy --branch main"
  },
  "dependencies": {
    "@astrojs/check": "^0.7.0",
    "@astrojs/react": "^3.5.0",
    "@astrojs/starlight": "^0.24.1",
    "@expressive-code/plugin-collapsible-sections": "^0.35.3",
    "@expressive-code/plugin-line-numbers": "^0.35.3",
    "@fontsource/ibm-plex-sans": "^5.0.20",
    "astro": "^4.10.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "starlight-links-validator": "^0.9.0",
    "typescript": "^5.4.5"
  },
  "devDependencies": {
    "@types/picomatch": "^2.3.3",
    "@types/react": "18.3.3",
    "@types/react-dom": "18.3.0",
    "likec4": "workspace:*",
    "npm-run-all2": "^6.1.2",
    "tsx": "~4.9.3"
  }
}
