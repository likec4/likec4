{
  "name": "@likec4/docs-astro",
  "type": "module",
  "version": "1.26.2",
  "packageManager": "pnpm@10.6.5",
  "private": true,
  "scripts": {
    "gupdate-theme-c4": "tsx scripts/generate-theme-c4.mts",
    "generate:types": "astro sync",
    "generate:dynamic": "likec4 gen react -o src/components/dynamic-view/likec4-views.jsx ./src/components/dynamic-view",
    "generate:theme": "likec4 gen react -o src/components/likec4-theme/likec4-views.jsx ./src/components/likec4-theme",
    "generate:index": "likec4 gen react -o src/components/index-page/likec4-views.jsx ./src/components/index-page",
    "generate": "run-p \"generate:*\"",
    "dev": "astro dev",
    "start": "astro dev",
    "typecheck": "astro check",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "deploy": "npx wrangler pages deploy dist --commit-dirty=true --project-name likec4-docs-astro",
    "deploy:prod": "run deploy --branch main",
    "clean": "rm -r -f \"dist\" \".astro\" \"node_modules\""
  },
  "dependencies": {
    "@astrojs/check": "catalog:astro",
    "@astrojs/react": "catalog:astro",
    "@astrojs/starlight": "catalog:astro",
    "@expressive-code/plugin-collapsible-sections": "catalog:astro",
    "@expressive-code/plugin-line-numbers": "catalog:astro",
    "@fontsource/ibm-plex-sans": "^5.2.5",
    "astro": "catalog:astro",
    "astro-og-canvas": "catalog:astro",
    "canvaskit-wasm": "^0.39.1",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "sharp": "^0.33.5",
    "starlight-heading-badges": "catalog:astro",
    "starlight-links-validator": "catalog:astro",
    "starlight-package-managers": "catalog:astro"
  },
  "devDependencies": {
    "@likec4/core": "workspace:*",
    "@types/picomatch": "^2.3.4",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "likec4": "workspace:*",
    "npm-run-all2": "^7.0.1",
    "tsx": "catalog:",
    "typescript": "catalog:"
  }
}
