{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "outputs": [
        ".astro/**"
      ]
    },
    "build": {
      "dependsOn": [
        "generate",
        "likec4#build-cli",
        "likec4#build-react"
      ],
      "outputs": [
        ".astro/**",
        "dist/**"
      ]
    }
  }
}
