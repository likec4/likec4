---
import { LikeC4View as C4View, type LikeC4ViewId } from './likec4-views'
interface Props {
  viewId: LikeC4ViewId;
}
const { viewId } = Astro.props
---

<C4View
  className="likec4-bigbank-view"
  viewId={viewId}
  injectFontCss={false}
  client:only="react">
</C4View>

<style>
  .likec4-bigbank-view {
    max-width: 800px;
    margin-left: auto;
    margin-right: auto;
  }
</style>
