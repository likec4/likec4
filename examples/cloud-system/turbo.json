{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "generate": {
      "dependsOn": ["@likec4/cli#build"],
      "outputs": [
        "src/**/*.generated.ts",
        "src/**/*.generated.tsx"
      ],
      "inputs": [
        "**/*.c4"
      ]
    },
    "compile": {
      "dependsOn": ["generate", "^build"],
      "outputs": []
    }
  }
}
