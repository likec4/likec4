{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@likec4/tsconfig/base.json",
  "compilerOptions": {
    "noEmit": true,
    "module": "esnext",
    "resolvePackageJsonExports": true,
    "customConditions": [
      "sources"
    ],
    "lib": [
      "ES2022",
      "WebWorker"
    ],
    "target": "ES2022",
    "noUnusedParameters": false
  },
  "include": [
    "./src"
  ]
}
