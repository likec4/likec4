{
  "name": "@likec4/layouts",
  "license": "MIT",
  "version": "1.27.3",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "files": [
    "dist",
    "!dist/src",
    "!**/__fixtures__/",
    "!**/*.spec.*",
    "!**/*.snap",
    "!**/*.map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/layouts"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./src/index.ts",
    "./graphviz/binary": "./src/graphviz/binary/index.ts"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "tsc --outDir dist",
    "lint": "run -T eslint src/ --fix",
    "test": "vitest run --no-isolate",
    "test:watch": "vitest",
    "clean": "rm -r -f -g \"dist/**\""
  },
  "dependencies": {
    "@hpcc-js/wasm-graphviz": "catalog:utils",
    "@likec4/core": "workspace:*",
    "@likec4/log": "workspace:*",
    "fast-equals": "catalog:utils",
    "graphology": "^0.25.4",
    "graphology-types": "^0.24.8",
    "indent-string": "^5.0.0",
    "khroma": "^2.1.0",
    "nano-spawn": "^0.2.0",
    "p-limit": "^6.2.0",
    "remeda": "catalog:utils",
    "strip-indent": "^4.0.0",
    "ts-graphviz": "^1.8.2",
    "type-fest": "catalog:utils",
    "which": "^5.0.0",
    "word-wrap": "^1.2.5"
  },
  "devDependencies": {
    "@likec4/tsconfig": "workspace:*",
    "@types/node": "catalog:",
    "@types/which": "^3.0.4",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "vitest": "catalog:vitest"
  },
  "packageManager": "pnpm@10.7.0"
}
