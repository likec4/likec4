{
  "$schema": "https://turbo.build/schema.json",
  "extends": ["//"],
  "pipeline": {
    "copy": {
      "dependsOn": ["@likec4/vscode-preview#build"],
      "outputs": ["dist/preview/**"]
    },
    "build": {
      "dependsOn": ["copy", "^build"],
      "outputs": ["dist/**", "!dist/preview/**"]
    }
  }
}
