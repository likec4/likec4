{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "outputs": [
        "src/meta.ts"
      ],
      "inputs": [
        "package.json"
      ],
      "outputLogs": "new-only"
    },
    "build": {
      "dependsOn": [
        "generate",
        "^generate",
        "@likec4/vscode-preview#build"
      ],
      "cache": false,
      "outputLogs": "new-only"
    }
  }
}
