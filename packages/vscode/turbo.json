{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build": {
      "inputs": [
        "$TURBO_DEFAULT$",
        "!data/**"
      ],
      "dependsOn": [
        "^generate",
        "topo",
        "@likec4/vscode-preview#build"
      ],
      "outputs": [
        "dist/**",
        "!dist/src/**",
        "!**/*.metafile.json"
      ]
    }
  }
}
