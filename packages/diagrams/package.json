{
  "name": "@likec4/diagrams",
  "version": "0.40.0",
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/diagrams"
  },
  "files": [
    "dist"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.js",
  "types": "./dist/cjs/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.ts",
        "default": "./dist/cjs/index.js"
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "typecheck": "tsc --emitDeclarationOnly",
    "build:turbo": "run -T turbo run build --filter='diagrams'",
    "build:watch": "unbuild",
    "build": "unbuild",
    "dev:watch": "run -T turbo run build:watch --filter='diagrams^...' & ladle serve",
    "dev": "run generate && ladle serve",
    "stories:generate": "node ./.scripts/generate-view-stories.mjs",
    "stories:build": "ladle build",
    "stories:preview": "ladle preview -p 61107",
    "lint": "run -T eslint src/ --fix",
    "clean": "run -T rimraf dist"
  },
  "dependencies": {
    "@likec4/core": "workspace:*",
    "@react-hookz/deep-equal": "^1.0.4",
    "@react-hookz/web": "^23.1.0",
    "@react-spring/konva": "^9.7.3",
    "@use-gesture/react": "^10.2.27",
    "body-scroll-lock-upgrade": "^1.0.4",
    "jotai": "^2.4.3",
    "khroma": "^2.0.0",
    "konva": "^9.2.1",
    "rambdax": "^9.1.1",
    "react-konva": "^18.2.10"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  }
}
