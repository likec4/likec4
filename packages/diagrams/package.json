{
  "name": "@likec4/diagrams",
  "version": "0.32.0",
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/diagrams"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./src": "./src/index.ts",
    "./src/diagram": "./src/diagram/index.ts",
    "./src/*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts",
      "./src/*/index.tsx"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "generate:full": "yarn workspace @likec4/cli run build && run generate",
    "generate": "yarn workspace @likec4/cli run likec4 codegen views-data $INIT_CWD/src-dev/likec4",
    "turbo-build": "run -T turbo run build --filter='diagrams'",
    "compile": "tsc --noEmit",
    "build": "vite build",
    "dev": "run generate && vite",
    "lint": "run -T eslint src/ --fix",
    "clean": "run -T rimraf dist"
  },
  "dependencies": {
    "@likec4/core": "workspace:*",
    "@react-hookz/deep-equal": "^1.0.4",
    "@react-hookz/web": "^23.1.0",
    "@react-spring/konva": "^9.7.3",
    "khroma": "^2.0.0",
    "konva": "^9.2.0",
    "rambdax": "^9.1.1",
    "react-konva": "^18.2.10"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.0.4",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "typescript": "^5.1.6",
    "vite": "^4.4.8",
    "vite-plugin-watch-and-run": "^1.1.3"
  },
  "volta": {
    "node": "18.16.1",
    "yarn": "3.6.1"
  }
}
