{
  "name": "@likec4/diagrams",
  "version": "0.7.0",
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://like-c4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "files": [
    "dist",
    "src",
    "!**/*.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/diagrams"
  },
  "type": "module",
  "sideEffects": false,
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "module": "./dist/index.js",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./src/*": [
      "./src/*",
      "./src/*.ts",
      "./src/*.tsx",
      "./src/*/index.ts"
    ]
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "turbo-build": "run -T turbo run build --filter='diagrams...'",
    "compile": "tsc --emitDeclarationOnly",
    "build:ts": "yarn node esbuild.js",
    "build:tsd": "tsc --emitDeclarationOnly",
    "build": "run-s 'build:ts' 'build:tsd'",
    "dev:ts": "yarn node esbuild.js --watch",
    "dev:tsd": "run build:tsd --watch",
    "dev": "run-p 'dev:*'",
    "lint": "run -T eslint src/ --fix",
    "clean": "run -T rimraf dist"
  },
  "dependencies": {
    "@react-spring/konva": "^9.7.2",
    "konva": "^9.0.1",
    "react-konva": "^18.2.7"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@likec4/core": "workspace:*",
    "@radix-ui/colors": "^0.1.8",
    "@react-hookz/deep-equal": "^1.0.4",
    "@react-hookz/web": "^23.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.0.11",
    "clsx": "^1.2.1",
    "esbuild": "^0.17.18",
    "khroma": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "polished": "^4.2.2",
    "rambdax": "^9.1.1",
    "tilg": "^0.1.1",
    "tiny-invariant": "^1.3.1",
    "typescript": "^5.0.4"
  },
  "installConfig": {
    "hoistingLimits": "workspaces"
  }
}
