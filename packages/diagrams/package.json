{
  "name": "@likec4/diagrams",
  "version": "0.4.0",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://like-c4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "files": [
    "dist"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git"
  },
  "main": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "default": "./dist/index.js"
    },
    "./embedded": {
      "types": "./dist/embedded/index.d.ts",
      "import": "./dist/embedded/index.js",
      "default": "./dist/embedded/index.js"
    },
    "./browser": {
      "types": "./dist/browser/index.d.ts",
      "import": "./dist/browser/index.js",
      "default": "./dist/browser/index.js"
    },
    "./dist/index.css": "./dist/index.css"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "main": "dist/index.js",
    "types": "dist/index.d.ts"
  },
  "scripts": {
    "compile": "tsc --noEmit",
    "release:build": "run-s 'clean' 'build'",
    "build:ts": "yarn node esbuild.mjs",
    "build:tsd": "tsc -p tsconfig.build.json",
    "build": "run-s 'build:ts' 'build:tsd'",
    "dev:ts": "run build:ts --watch",
    "dev:tsd": "run build:tsd --watch",
    "dev": "run-p 'dev:*'",
    "lint": "run -T eslint src/ --fix",
    "clean": "run -T rimraf dist"
  },
  "dependencies": {
    "@react-spring/konva": "^9.7.2",
    "konva": "^8.4.3",
    "react-konva": "^18.2.7"
  },
  "peerDependencies": {
    "@likec4/core": "workspace:*",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "peerDependenciesMeta": {
    "@likec4/core": {
      "optional": true
    }
  },
  "devDependencies": {
    "@likec4/core": "workspace:*",
    "@radix-ui/colors": "^0.1.8",
    "@radix-ui/react-dialog": "^1.0.3",
    "@react-hookz/deep-equal": "^1.0.4",
    "@react-hookz/web": "^23.0.0",
    "@types/react": "^18.0.35",
    "@types/react-dom": "^18.0.11",
    "@vanilla-extract/css": "^1.11.0",
    "@vanilla-extract/dynamic": "^2.0.3",
    "@vanilla-extract/esbuild-plugin": "^2.2.2",
    "@vanilla-extract/recipes": "^0.4.0",
    "@vanilla-extract/sprinkles": "^1.5.2",
    "clsx": "^1.2.1",
    "esbuild": "^0.17.15",
    "khroma": "^2.0.0",
    "npm-run-all": "^4.1.5",
    "polished": "^4.2.2",
    "rambdax": "^9.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tiny-invariant": "^1.3.1",
    "typescript": "^5.0.4"
  }
}
