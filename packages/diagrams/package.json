{
  "name": "@likec4/diagrams",
  "version": "0.37.1",
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/diagrams"
  },
  "files": [
    "dist",
    "src"
  ],
  "type": "module",
  "sideEffects": false,
  "main": "./dist/index.cjs",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "main": "./dist/index.cjs",
    "types": "./dist/index.d.ts",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "require": "./dist/index.cjs"
      }
    }
  },
  "packageManager": "yarn@3.6.1",
  "scripts": {
    "typecheck": "unbuild && tsc --noEmit",
    "generate:rebuild": "yarn workspace @likec4/cli run build && run generate",
    "generate:likec4": "node ./.scripts/generate-theme-likec4.mjs",
    "generate": "../cli/bin/likec4 codegen views-data src/stories/likec4",
    "build:turbo": "run -T turbo run build --filter='diagrams'",
    "build:watch": "unbuild",
    "build": "unbuild",
    "dev:watch": "run -T turbo run build:watch --filter='diagrams^...' & ladle serve",
    "dev": "run generate && ladle serve",
    "stories:generate": "node ./.scripts/generate-view-stories.mjs",
    "stories:build": "ladle build",
    "stories:preview": "ladle preview -p 61107",
    "lint": "run -T eslint src/ --fix",
    "clean": "run -T rimraf dist"
  },
  "dependencies": {
    "@likec4/core": "workspace:*",
    "@react-hookz/deep-equal": "^1.0.4",
    "@react-hookz/web": "^23.1.0",
    "@react-spring/konva": "^9.7.3",
    "@use-gesture/react": "^10.2.27",
    "body-scroll-lock-upgrade": "^1.0.4",
    "jotai": "^2.4.2",
    "khroma": "^2.0.0",
    "konva": "^9.2.0",
    "rambdax": "^9.1.1",
    "react-konva": "^18.2.10"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@ladle/react": "^3.0.0",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "tilg": "^0.1.1",
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0"
  }
}
