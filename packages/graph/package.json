{
  "name": "@likec4/graph",
  "license": "MIT",
  "version": "0.40.0",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "files": [
    "dist",
    "!**/__mocks__/",
    "!**/*.spec.*",
    "!**/*.snap",
    "!**/*.map"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/graph"
  },
  "module": "src/index.ts",
  "types": "src/index.ts",
  "type": "module",
  "sideEffects": false,
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "typecheck:turbo": "run -T turbo run typecheck --filter='graph'",
    "typecheck": "tsc --emitDeclarationOnly",
    "build:turbo": "run -T turbo run build --filter='graph'",
    "build": "tsc",
    "dev": "tsc --watch",
    "lint": "run -T eslint src/ --fix",
    "test": "run -T vitest run",
    "test:watch": "run -T vitest",
    "clean": "run -T rimraf dist"
  },
  "dependencies": {
    "@dagrejs/graphlib": "^2.1.13",
    "@likec4/core": "workspace:*",
    "rambdax": "^9.1.1",
    "remeda": "^1.27.1"
  },
  "devDependencies": {
    "typescript": "^5.2.2",
    "unbuild": "^2.0.0",
    "vitest": "^0.34.6"
  },
  "packageManager": "yarn@3.6.4",
  "volta": {
    "extends": "../../package.json"
  }
}
