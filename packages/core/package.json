{
  "name": "@likec4/core",
  "version": "1.47.0",
  "license": "MIT",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "description": "A core package for LikeC4, containing types, api, utilities and guards.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/core"
  },
  "bugs": "https://github.com/likec4/likec4/issues",
  "scripts": {
    "typecheck": "tsc -b --verbose",
    "build": "unbuild",
    "lint": "run -T eslint src/ --fix",
    "lint:package": "pnpx publint ./package.tgz",
    "test": "vitest run --no-isolate",
    "test:watch": "vitest",
    "clean": "likec4ops clean",
    "prepack": "likec4ops prepack",
    "pack": "pnpm pack",
    "postpack": "cp likec4-core-$npm_package_version.tgz package.tgz || true"
  },
  "files": [
    "dist",
    "**/package.json",
    "!**/*.spec.ts",
    "!**/*.test-d.ts",
    "!**/__test__/*",
    "!**/__snapshots__/*",
    "!**/*.map"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "default": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs",
        "require": "./dist/index.cjs",
        "default": "./dist/index.mjs"
      }
    },
    "./builder": {
      "sources": "./src/builder/index.ts",
      "default": {
        "types": "./dist/builder/index.d.mts",
        "import": "./dist/builder/index.mjs",
        "require": "./dist/builder/index.cjs",
        "default": "./dist/builder/index.mjs"
      }
    },
    "./geometry": {
      "sources": "./src/geometry/index.ts",
      "default": {
        "types": "./dist/geometry/index.d.mts",
        "import": "./dist/geometry/index.mjs",
        "require": "./dist/geometry/index.cjs",
        "default": "./dist/geometry/index.mjs"
      }
    },
    "./compute-view/relationships": {
      "sources": "./src/compute-view/relationships-view/index.ts",
      "default": {
        "types": "./dist/compute-view/relationships-view/index.d.mts",
        "import": "./dist/compute-view/relationships-view/index.mjs",
        "require": "./dist/compute-view/relationships-view/index.cjs",
        "default": "./dist/compute-view/relationships-view/index.mjs"
      }
    },
    "./compute-view": {
      "sources": "./src/compute-view/index.ts",
      "default": {
        "types": "./dist/compute-view/index.d.mts",
        "import": "./dist/compute-view/index.mjs",
        "require": "./dist/compute-view/index.cjs",
        "default": "./dist/compute-view/index.mjs"
      }
    },
    "./model": {
      "sources": "./src/model/index.ts",
      "default": {
        "types": "./dist/model/index.d.mts",
        "import": "./dist/model/index.mjs",
        "require": "./dist/model/index.cjs",
        "default": "./dist/model/index.mjs"
      }
    },
    "./styles": {
      "sources": "./src/styles/index.ts",
      "default": {
        "types": "./dist/styles/index.d.mts",
        "import": "./dist/styles/index.mjs",
        "require": "./dist/styles/index.cjs",
        "default": "./dist/styles/index.mjs"
      }
    },
    "./types": {
      "sources": "./src/types/index.ts",
      "default": {
        "types": "./dist/types/index.d.mts",
        "import": "./dist/types/index.mjs",
        "require": "./dist/types/index.cjs",
        "default": "./dist/types/index.mjs"
      }
    },
    "./utils/graphology": {
      "sources": "./src/utils/graphology/index.ts",
      "default": {
        "types": "./dist/utils/graphology/index.d.mts",
        "import": "./dist/utils/graphology/index.mjs",
        "require": "./dist/utils/graphology/index.cjs",
        "default": "./dist/utils/graphology/index.mjs"
      }
    },
    "./utils": {
      "sources": "./src/utils/index.ts",
      "default": {
        "types": "./dist/utils/index.d.mts",
        "import": "./dist/utils/index.mjs",
        "require": "./dist/utils/index.cjs",
        "default": "./dist/utils/index.mjs"
      }
    },
    "./package.json": "./package.json"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "types": "dist/index.d.mts",
    "main": "dist/index.cjs",
    "module": "dist/index.mjs"
  },
  "dependencies": {
    "type-fest": "catalog:externals",
    "immer": "catalog:utils"
  },
  "devDependencies": {
    "@dagrejs/dagre": "catalog:",
    "@likec4/devops": "workspace:*",
    "@likec4/tsconfig": "workspace:*",
    "@likec4/style-preset": "workspace:*",
    "@mantine/colors-generator": "catalog:mantine",
    "@types/chroma-js": "catalog:utils",
    "@types/extend": "3.0.4",
    "@types/hast": "3.0.4",
    "@types/mdast": "4.0.4",
    "@types/natural-compare-lite": "catalog:utils",
    "@types/node": "catalog:",
    "@types/object-hash": "^3.0.6",
    "@types/unist": "3.0.3",
    "chroma-js": "catalog:utils",
    "defu": "catalog:utils",
    "fdir": "catalog:externals",
    "graphology": "catalog:",
    "graphology-dag": "catalog:",
    "graphology-types": "catalog:",
    "mdast-util-from-markdown": "^2.0.2",
    "mdast-util-to-string": "^4.0.0",
    "mdast-util-to-hast": "^13.2.1",
    "mnemonist": "0.40.3",
    "natural-compare-lite": "catalog:utils",
    "object-hash": "catalog:utils",
    "obliterator": "^2.0.5",
    "oxlint": "catalog:",
    "rehype-format": "5.0.1",
    "rehype-sanitize": "^6.0.0",
    "rehype-stringify": "10.0.1",
    "remark-gfm": "4.0.1",
    "remark-github-blockquote-alert": "^2.0.1",
    "remark-parse": "11.0.0",
    "remark-rehype": "11.1.2",
    "remeda": "catalog:utils",
    "turbo": "catalog:",
    "typescript": "catalog:",
    "unbuild": "catalog:",
    "unified": "11.0.5",
    "vitest": "catalog:vitest"
  }
}
