{
  "name": "@likec4/core",
  "version": "1.19.2",
  "license": "MIT",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/core"
  },
  "bugs": "https://github.com/likec4/likec4/issues",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "unbuild",
    "lint": "run -T eslint src/ --fix",
    "test": "vitest run --no-isolate",
    "test:watch": "vitest",
    "clean": "rm -r -f dist"
  },
  "files": [
    "dist",
    "src",
    "!**/*.spec.ts",
    "!**/__tests__/*",
    "!**/*.map"
  ],
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    },
    "./builder": {
      "sources": "./src/builder/index.ts",
      "default": {
        "types": "./dist/builder/index.d.ts",
        "import": "./dist/builder/index.js",
        "default": "./dist/builder/index.js"
      }
    },
    "./compute-view": {
      "sources": "./src/compute-view/index.ts",
      "default": {
        "types": "./dist/compute-view/index.d.ts",
        "import": "./dist/compute-view/index.js",
        "default": "./dist/compute-view/index.js"
      }
    },
    "./model": {
      "sources": "./src/model/index.ts",
      "default": {
        "types": "./dist/model/index.d.ts",
        "import": "./dist/model/index.js",
        "default": "./dist/model/index.js"
      }
    },
    "./utils": {
      "sources": "./src/utils/index.ts",
      "default": {
        "types": "./dist/utils/index.d.ts",
        "import": "./dist/utils/index.js",
        "default": "./dist/utils/index.js"
      }
    },
    "./types": {
      "sources": "./src/types/index.ts",
      "default": {
        "types": "./dist/types/index.d.ts",
        "import": "./dist/types/index.js",
        "default": "./dist/types/index.js"
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "dependencies": {
    "type-fest": "4.28.1"
  },
  "devDependencies": {
    "@likec4/tsconfig": "workspace:*",
    "@mantine/colors-generator": "7.15.2",
    "@types/natural-compare-lite": "^1.4.2",
    "@types/object-hash": "^3.0.6",
    "chroma-js": "^3.1.2",
    "defu": "^6.1.4",
    "graphology": "^0.25.4",
    "graphology-dag": "^0.4.1",
    "graphology-types": "^0.24.8",
    "mnemonist": "^0.39.8",
    "natural-compare-lite": "^1.4.0",
    "object-hash": "^3.0.0",
    "obliterator": "^2.0.4",
    "remeda": "^2.17.4",
    "turbo": "^2.3.3",
    "typescript": "^5.7.2",
    "unbuild": "^3.1.0",
    "vitest": "^2.1.8"
  },
  "packageManager": "yarn@4.6.0"
}
