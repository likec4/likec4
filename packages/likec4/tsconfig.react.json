{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@likec4/tsconfig/react-lib.json",
  "compilerOptions": {
    "composite": true,
    "tsBuildInfoFile": "lib/react/.tsbuildinfo",
    "outDir": "lib/react",
    "rootDir": "react",
    "types": [
      "vite/client"
    ],
    "paths": {
      "likec4/model": [
        "./src/model/index.ts"
      ]
    },
  },
  "include": [
    "./react/**/*.ts",
  ],
  "references": [
    {
      "path": "./tsconfig.cli.json"
    },
    {
      "path": "../core/tsconfig.json"
    }
  ]
}
