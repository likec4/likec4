{
  "name": "likec4",
  "version": "0.40.0",
  "license": "MIT",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/likec4"
  },
  "engines": {
    "node": "^18.0.0 || >=20.0.0"
  },
  "bugs": "https://github.com/likec4/likec4/issues",
  "files": [
    "bin",
    "dist",
    "!dist/@likec4/**/*.d.ts",
    "!**/*.map"
  ],
  "type": "module",
  "bin": "./bin/likec4.mjs",
  "scripts": {
    "typecheck:turbo": "run -T turbo run typecheck --filter='likec4'",
    "build:turbo": "run -T turbo run build --filter='likec4'",
    "typecheck:app": "tsc --noEmit -p ./app/tsconfig.json",
    "typecheck:cli": "tsc --noEmit",
    "typecheck": "run 'typecheck:cli' && run 'typecheck:app'",
    "build:watch": "tsc --watch",
    "build": "$npm_node_execpath esbuild.mjs",
    "prepack": "NODE_ENV=production run clean && run build",
    "lint": "run -T eslint src/ --fix",
    "test": "run -T vitest run",
    "test:watch": "run -T vitest",
    "clean": "run -T rimraf dist app/dist dev/.export dev/dist",
    "cli:serve": "tsx watch src/cli/index.ts serve ./dev",
    "cli:build": "tsx src/cli/index.ts build ./dev",
    "cli:preview": "tsx src/cli/index.ts preview ./dev",
    "cli:export": "tsx src/cli/index.ts export png -o dev/.export dev"
  },
  "dependencies": {
    "@dagrejs/graphlib": "^2.1.13",
    "@nanostores/logger": "^0.2.3",
    "@nanostores/react": "^0.7.1",
    "@nanostores/router": "^0.11.0",
    "@radix-ui/react-icons": "^1.3.0",
    "@radix-ui/themes": "^2.0.0",
    "@react-hookz/deep-equal": "^1.0.4",
    "@react-hookz/web": "^23.1.0",
    "@react-spring/konva": "^9.7.3",
    "@use-gesture/react": "^10.3.0",
    "@vitejs/plugin-react": "^4.1.0",
    "autoprefixer": "^10.4.16",
    "body-scroll-lock-upgrade": "^1.0.4",
    "classnames": "^2.3.2",
    "clsx": "^1.2.1",
    "jotai": "^2.4.3",
    "khroma": "^2.0.0",
    "konva": "^9.2.1",
    "nanostores": "^0.9.3",
    "playwright-core": "^1.38.1",
    "postcss": "^8.4.31",
    "prop-types": "^15.8.1",
    "radix-themes-tw": "^0.0.8",
    "rambdax": "^9.1.1",
    "react": "^18.2.0",
    "react-accessible-treeview": "^2.8.0",
    "react-dom": "^18.2.0",
    "react-konva": "^18.2.10",
    "remeda": "^1.27.1",
    "safe-stable-stringify": "^2.4.3",
    "tailwind-merge": "^1.14.0",
    "tailwindcss": "^3.3.3",
    "tailwindcss-animate": "^1.0.7",
    "ts-custom-error": "^3.3.1",
    "vite": "^4.4.11"
  },
  "devDependencies": {
    "@likec4/core": "workspace:*",
    "@likec4/diagrams": "workspace:*",
    "@likec4/generators": "workspace:*",
    "@likec4/language-server": "workspace:*",
    "@likec4/layouts": "workspace:*",
    "@types/node": "^18.15.11",
    "@types/prop-types": "^15",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/yargs": "^17.0.28",
    "boxen": "^7.1.1",
    "esbuild": "^0.19.4",
    "esbuild-node-externals": "^1.9.0",
    "execa": "^8.0.1",
    "get-port": "^7.0.0",
    "jotai-devtools": "^0.6.3",
    "json5": "^2.2.3",
    "mkdirp": "^3.0.1",
    "p-queue": "^7.4.1",
    "picocolors": "^1.0.0",
    "pretty-ms": "^8.0.0",
    "strip-indent": "^4.0.0",
    "tsx": "^3.13.0",
    "typescript": "^5.2.2",
    "typescript-plugin-css-modules": "^5.0.1",
    "vitest": "^0.34.6",
    "yargs": "^17.7.2"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "packageManager": "yarn@3.6.4"
}
