{
  "name": "likec4",
  "version": "1.24.0",
  "license": "MIT",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "sponsor": {
    "url": "https://github.com/sponsors/davydkov"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/likec4"
  },
  "engines": {
    "node": ">=20.18.3"
  },
  "engineStrict": true,
  "bugs": "https://github.com/likec4/likec4/issues",
  "type": "module",
  "bin": "./bin/likec4.mjs",
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "default": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      }
    },
    "./model": {
      "sources": "./src/model/index.ts",
      "default": {
        "types": "./dist/model/index.d.mts",
        "import": "./dist/model/index.mjs",
        "default": "./dist/model/index.mjs"
      }
    },
    "./react": {
      "sources": "./app/react/components/index.ts",
      "default": {
        "types": "./react/index.d.ts",
        "import": "./react/index.js",
        "default": "./react/index.js"
      }
    },
    "./react/style.css": "./react/style.css",
    "./icons/all": {
      "types": "./icons/all.d.ts",
      "import": "./icons/all.js",
      "default": "./icons/all.js"
    },
    "./icons/*": {
      "types": "./icons/icon.d.ts",
      "import": "./icons/*",
      "default": "./icons/*"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "turbo-build": "turbo run build --log-prefix=none --log-order=grouped",
    "package": "yarn pack",
    "typecheck:app": "tsc --noEmit -p ./app/tsconfig.json",
    "typecheck:cli": "tsc --noEmit -p ./tsconfig.cli.json",
    "typecheck": "run-p --print-label 'typecheck:*'",
    "build-cli": "unbuild",
    "build-app": "run-p --print-label 'bundle:*'",
    "bundle:app": "tsx scripts/bundle-app.ts",
    "bundle:react": "tsx scripts/bundle-react.ts",
    "bundle:webcomponent": "tsx scripts/bundle-webcomponent.ts",
    "generate": "tsx scripts/generate.ts",
    "prepack": "turbo run build --log-prefix=none --log-order=grouped",
    "pack": "pnpm pack",
    "postpack": "cp likec4-$npm_package_version.tgz package.tgz || true",
    "lint": "run -T eslint src/ --fix",
    "clean": "rm -r -f dist app/dist dev/.export dev/dist node_modules/.vite",
    "start": "tsx --conditions=sources src/cli/index.ts",
    "dev": "pnpm cli:serve dev",
    "dev:docs:bigbank": "pnpm cli:serve ../../apps/docs/src/components/bigbank",
    "dev:docs:dynamic-view": "pnpm cli:serve ../../apps/docs/src/components/dynamic-view",
    "dev:docs:index-page": "pnpm cli:serve ../../apps/docs/src/components/index-page",
    "dev:docs:theme": "pnpm cli:serve ../../apps/docs/src/components/likec4-theme",
    "dev:playground:deployment": "pnpm cli:serve ../../apps/playground/src/examples/deployment",
    "dev:template": "pnpm cli:serve ../create-likec4/template",
    "dev:e2e": "pnpm cli:serve ../../e2e/src",
    "dev:example-cloud": "pnpm cli:serve ../../examples/cloud-system",
    "dev:failed": "pnpm cli:serve ../../examples/failed",
    "dev:example-dev": "pnpm cli:serve ../../examples/diagrams-dev/likec4",
    "cli": "tsx --conditions=sources src/cli/index.ts",
    "cli:serve": "tsx watch --conditions=sources src/cli/index.ts serve",
    "cli:build": "run cli build -o dev/dist dev",
    "cli:preview": "run cli preview -o dev/dist dev",
    "cli:export": "run cli export png -o dev/export dev",
    "cli:export:json": "run cli export json -o dev/export/likec4.json dev",
    "test": "vitest run --no-isolate",
    "vitest:ui": "vitest --no-isolate --ui",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@hpcc-js/wasm-graphviz": "1.7.0",
    "@likec4/core": "workspace:~",
    "@vitejs/plugin-react": "4.3.4",
    "boxen": "^8.0.1",
    "playwright": "1.50.1",
    "react": "~19.0.0",
    "react-dom": "~19.0.0",
    "rollup": "4.34.6",
    "type-fest": "4.34.1",
    "vite": "5.4.14"
  },
  "devDependencies": {
    "@dagrejs/dagre": "1.1.4",
    "@fontsource/ibm-plex-sans": "^5.1.1",
    "@likec4/diagram": "workspace:*",
    "@likec4/generators": "workspace:*",
    "@likec4/icons": "workspace:*",
    "@likec4/language-server": "workspace:*",
    "@likec4/layouts": "workspace:*",
    "@likec4/log": "workspace:*",
    "@likec4/tsconfig": "workspace:*",
    "@mantine/core": "7.17.0",
    "@mantine/hooks": "7.17.0",
    "@mantine/vanilla-extract": "7.17.0",
    "@nanostores/react": "0.8.4",
    "@react-hookz/web": "^25.0.1",
    "@tabler/icons-react": "3.29.0",
    "@tanstack/react-router": "1.97.17",
    "@tanstack/router-cli": "1.97.17",
    "@tanstack/router-vite-plugin": "1.97.17",
    "@types/node": "^20.17.17",
    "@types/prop-types": "^15.7.14",
    "@types/react": "~19.0.8",
    "@types/react-dom": "~19.0.3",
    "@types/semver": "^7.5.8",
    "@types/yargs": "^17.0.33",
    "@vanilla-extract/css": "^1.17.1",
    "@vanilla-extract/dynamic": "^2.1.2",
    "@vanilla-extract/vite-plugin": "^5.0.1",
    "@xyflow/react": "12.4.3",
    "@xyflow/system": "0.0.51",
    "autoprefixer": "^10.4.20",
    "classnames": "^2.5.1",
    "clsx": "^2.1.1",
    "conf": "^13.1.0",
    "consola": "^3.4.0",
    "defu": "^6.1.4",
    "dts-bundle-generator": "^9.5.1",
    "esbuild": "^0.25.0",
    "esbuild-node-externals": "^1.16.0",
    "esm-env": "^1.2.2",
    "fast-equals": "^5.2.2",
    "fdir": "^6.4.3",
    "framer-motion": "^12.4.2",
    "get-port": "^7.1.0",
    "glob": "^11.0.1",
    "html-to-image": "^1.11.11",
    "json5": "^2.2.3",
    "ky": "^1.7.5",
    "langium": "3.3.1",
    "merge-error-cause": "^5.0.0",
    "mkdirp": "^3.0.1",
    "nano-spawn": "^0.2.0",
    "nanostores": "0.11.3",
    "npm-run-all2": "^7.0.1",
    "p-limit": "^6.2.0",
    "package-up": "^5.0.0",
    "postcss": "^8.4.49",
    "postcss-preset-mantine": "^1.17.0",
    "pretty-ms": "^9.2.0",
    "prop-types": "^15.8.1",
    "react-error-boundary": "^5.0.0",
    "react-resizable-panels": "^2.1.7",
    "react-shadow": "^20.6.0",
    "remeda": "^2.20.1",
    "semver": "^7.7.1",
    "std-env": "^3.8.0",
    "strip-indent": "^4.0.0",
    "terser": "^5.38.1",
    "tinyrainbow": "^2.0.0",
    "tsx": "~4.19.2",
    "turbo": "^2.4.2",
    "typescript": "catalog:",
    "ufo": "^1.5.4",
    "unbuild": "^3.3.1",
    "vite-plugin-css-injected-by-js": "^3.5.2",
    "vite-plugin-dts": "^4.5.0",
    "vite-plugin-shadow-style": "^1.2.0",
    "vite-plugin-singlefile": "^2.1.0",
    "vscode-jsonrpc": "8.2.0",
    "vscode-languageserver": "9.0.1",
    "vscode-uri": "3.1.0",
    "vscode-languageserver-types": "3.17.5",
    "vitest": "^3.0.4",
    "which": "^5.0.0",
    "yargs": "17.7.2"
  },
  "packageManager": "pnpm@10.4.1"
}
