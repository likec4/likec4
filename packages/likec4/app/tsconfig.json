{
  "extends": "@likec4/tsconfig/react-lib.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "noPropertyAccessFromIndexSignature": false,
    "module": "Preserve",
    "moduleResolution": "bundler",
    "tsBuildInfoFile": "../lib/app/.tsbuildinfo",
    "outDir": "../lib/app",
    "rootDir": ".",
    "types": [
      "vite/client"
    ],
    "paths": {
      "likec4/react": [
        "../react/index.ts"
      ]
    },
  },
  "include": [
    "src",
    "react",
    "webcomponent"
  ],
  "references": [
    {
      "path": "../tsconfig.react.json"
    }
  ]
}
