{
  "extends": "@likec4/tsconfig/react-lib.json",
  "compilerOptions": {
    "lib": [
      "DOM",
      "DOM.Iterable",
      "ESNext"
    ],
    "noPropertyAccessFromIndexSignature": false,
    "module": "Preserve",
    "moduleResolution": "bundler",
    "tsBuildInfoFile": "../lib/app/.tsbuildinfo",
    "outDir": "../lib/app",
    "rootDir": ".",
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "./src",
    "./react",
    "./webcomponent"
  ],
  "references": [
    {
      "path": "../tsconfig.cli.json"
    },
    {
      "path": "../tsconfig.react.json"
    },
    {
      "path": "../../core/"
    },
    {
      "path": "../../diagram/tsconfig.src.json"
    },
    {
      "path": "../../../styled-system/styles/"
    }
  ]
}
