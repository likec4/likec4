{
  "name": "@likec4/generators",
  "version": "1.49.0",
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "description": "Converts LikeC4 Model to other formats, like Mermaid, PlantUML, SVG, etc. Also generates typings for Model API and React",
  "files": [
    "dist",
    "src",
    "**/package.json",
    "!**/__*/**",
    "!**/*.spec.*",
    "!**/*.snap",
    "!**/*.map"
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/generators"
  },
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "default": {
        "types": "./dist/index.d.mts",
        "import": "./dist/index.mjs",
        "default": "./dist/index.mjs"
      }
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "types": "dist/index.d.mts",
    "module": "dist/index.mjs"
  },
  "scripts": {
    "typecheck": "tsc -b --verbose",
    "build": "obuild",
    "lint": "run -T eslint src/ --fix",
    "clean": "likec4ops clean",
    "prepack": "likec4ops prepack",
    "pack": "pnpm pack",
    "postpack": "cp likec4-generators-$npm_package_version.tgz package.tgz || true",
    "test": "vitest run --no-isolate",
    "test:watch": "vitest"
  },
  "dependencies": {
    "@likec4/core": "workspace:*",
    "@likec4/log": "workspace:*",
    "remeda": "catalog:utils",
    "json5": "catalog:utils",
    "langium": "catalog:langium"
  },
  "devDependencies": {
    "@likec4/tsconfig": "workspace:*",
    "@likec4/devops": "workspace:*",
    "@types/node": "catalog:",
    "typescript": "catalog:",
    "obuild": "catalog:obuild",
    "vitest": "catalog:vitest"
  }
}
