{
  "name": "@likec4/icons",
  "version": "1.42.1",
  "license": "MIT",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "description": "Bundled React SVG icons, used in LikeC4 Diagram",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/icons"
  },
  "bugs": "https://github.com/likec4/likec4/issues",
  "type": "module",
  "types": "all.d.ts",
  "module": "all.js",
  "sideEffects": false,
  "exports": {
    ".": {
      "types": "./all.d.ts",
      "default": "./all.js"
    },
    "./all": {
      "types": "./all.d.ts",
      "default": "./all.js"
    },
    "./aws": {
      "default": "./aws/index.js"
    },
    "./aws/*": {
      "types": "./icon.d.ts",
      "default": "./aws/*.js"
    },
    "./tech": {
      "default": "./tech/index.js"
    },
    "./tech/*": {
      "types": "./icon.d.ts",
      "default": "./tech/*.js"
    },
    "./gcp": {
      "default": "./gcp/index.js"
    },
    "./gcp/*": {
      "types": "./icon.d.ts",
      "default": "./gcp/*.js"
    },
    "./azure": {
      "default": "./azure/index.js"
    },
    "./azure/*": {
      "types": "./icon.d.ts",
      "default": "./azure/*.js"
    },
    "./*": {
      "types": "./icon.d.ts",
      "default": "./tech/*.js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "update-icons": "pnpm node scripts/update-icons.mjs && pnpm run generate",
    "clean": "likec4ops clean aws azure gcp tech .tmp",
    "postclean": "pnpm run update-icons",
    "generate": "pnpm node scripts/generate.mjs",
    "prepack": "likec4ops prepack",
    "pack": "pnpm pack",
    "postpack": "cp likec4-icons-$npm_package_version.tgz package.tgz || true",
    "lint:package": "pnpx publint ./package.tgz"
  },
  "peerDependencies": {
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "devDependencies": {
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "@likec4/devops": "workspace:*",
    "esbuild": "catalog:esbuild",
    "fdir": "catalog:utils",
    "picomatch": "catalog:utils",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "typescript": "catalog:",
    "wireit": "catalog:",
    "zx": "catalog:"
  }
}
