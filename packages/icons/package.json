{
  "name": "@likec4/icons",
  "version": "1.35.0",
  "license": "MIT",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/icons"
  },
  "bugs": "https://github.com/likec4/likec4/issues",
  "type": "module",
  "sideEffects": false,
  "exports": {
    ".": "./tech/index.js",
    "./all": {
      "types": "./all.d.ts",
      "default": "./all.js"
    },
    "./aws": "./aws/index.js",
    "./aws/*": {
      "types": "./icon.d.ts",
      "default": "./aws/*.js"
    },
    "./tech": "./tech/index.js",
    "./tech/*": {
      "types": "./icon.d.ts",
      "default": "./tech/*.js"
    },
    "./gcp": "./gcp/index.js",
    "./gcp/*": {
      "types": "./icon.d.ts",
      "default": "./gcp/*.js"
    },
    "./azure": "./azure/index.js",
    "./azure/*": {
      "types": "./icon.d.ts",
      "default": "./azure/*.js"
    },
    "./*": {
      "types": "./icon.d.ts",
      "default": "./tech/*.js"
    }
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "scripts": {
    "update-icons": "pnpm node scripts/update-icons.mjs && pnpm run generate",
    "generate": "pnpm node scripts/generate.mjs",
    "pack": "pnpm pack",
    "postpack": "cp likec4-icons-$npm_package_version.tgz package.tgz || true",
    "lint:package": "pnpx publint ./package.tgz"
  },
  "peerDependencies": {
    "react": "^18.x || ^19.x",
    "react-dom": "^18.x || ^19.x"
  },
  "devDependencies": {
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react",
    "esbuild": "catalog:esbuild",
    "execa": "^9.5.3",
    "fdir": "catalog:utils",
    "picomatch": "catalog:utils",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "typescript": "catalog:",
    "wireit": "catalog:"
  }
}
