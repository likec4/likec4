{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "build:dist": {
      "dependsOn": [
        "^generate",
        "topo"
      ],
      "outputs": [
        "dist/**"
      ],
      "outputLogs": "new-only"
    },
    "build:bundle": {
      "dependsOn": [
        "^generate",
        "topo"
      ],
      "outputs": [
        "bundle/**"
      ],
      "outputLogs": "new-only"
    },
    "build": {
      "dependsOn": [
        "build:dist",
        "build:bundle"
      ],
      "inputs": [],
      "outputs": []
    }
  }
}
