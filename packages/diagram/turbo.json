{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "dependsOn": [
        "^generate"
      ],
      "outputs": [
        "styled-system/**"
      ],
      "outputLogs": "new-only"
    },
    "build:dist": {
      "dependsOn": [
        "generate",
        "@likec4/core#build"
      ],
      "outputs": [
        "dist/**"
      ],
      "outputLogs": "new-only"
    },
    "build:bundle": {
      "dependsOn": [
        "generate",
        "topo"
      ],
      "outputs": [
        "bundle/**"
      ],
      "outputLogs": "new-only"
    },
    "build": {
      "dependsOn": [
        "build:dist",
        "build:bundle"
      ],
      "outputLogs": "new-only"
    }
  }
}
