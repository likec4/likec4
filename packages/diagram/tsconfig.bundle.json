{
  "compilerOptions": {
    "noEmitOnError": false,
    "declarationMap": false,
    "esModuleInterop": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "Bundler",
    "jsx": "react-jsx",
    "skipDefaultLibCheck": true,
    "skipLibCheck": true,
    "strict": false,
    "alwaysStrict": false,
    "customConditions": [
      "sources",
    ],
    "checkJs": false,
    "forceConsistentCasingInFileNames": true,
    "baseUrl": "."
  },
  "files": [
    "./src/bundle/index.ts",
  ]
}
