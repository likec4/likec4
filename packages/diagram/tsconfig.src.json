{
  "$schema": "https://json.schemastore.org/tsconfig",
  "extends": "@likec4/tsconfig/react-lib.json",
  "compilerOptions": {
    "customConditions": [
      "sources"
    ],
    "composite": true,
    "rootDir": "src",
    "types": [
      "vite/client"
    ]
  },
  "include": [
    "src",
    "src/**/*.json"
  ],
  "exclude": [
    "src/bundle/react-dom-server-mock.ts"
  ],
  "references": [
    {
      "path": "../core/"
    },
    {
      "path": "../../styled-system/preset/"
    },
    {
      "path": "../../styled-system/styles/"
    }
  ]
}
