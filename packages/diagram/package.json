  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/diagram"
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "dist",
    "!**/*.spec.*",
    "!**/*.map"
  ],
  "type": "module",
  "exports": {
    ".": "./src/index.ts",
    "./style.css": "./dist/style.css",
    "./bundle": "./dist/bundle.js"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public",
    "exports": {
      ".": {
        "types": "./dist/index.d.ts",
        "default": "./dist/index.js"
      },
      "./style.css": "./dist/style.css",
      "./bundle": {
        "types": "./dist/bundle.d.ts",
        "default": "./dist/bundle.js"
      }
    }
  },
  "packageManager": "yarn@4.0.2",
  "scripts": {
    "typecheck": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "build:turbo": "run -T turbo build --filter='diagram'",
    "build": "vite build && run typecheck",
    "lint": "run -T eslint src/ --fix",
    "clean": "run -T rimraf dist bundle"
  },
  "peerDependencies": {
    "@mantine/core": "^7.7.0",
    "@mantine/hooks": "^7.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "scheduler": "^0.23.0"
  },
  "peerDependenciesMeta": {
    "@mantine/core": {
      "optional": true
    },
    "@mantine/hooks": {
      "optional": true
    },
    "scheduler": {
      "optional": true
    }
  },
  "devDependencies": {
    "@likec4/core": "workspace:*",
