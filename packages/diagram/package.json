{
  "name": "@likec4/diagram",
  "version": "1.0.0-next.11",
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "https://github.com/likec4/likec4.git",
    "directory": "packages/diagram"
  },
  "files": [
    "package.json",
    "LICENSE",
    "README.md",
    "dist",
    "!**/*.spec.*",
    "!**/*.d.ts.map"
  ],
  "sideEffects": [
    "**/*.css"
  ],
  "type": "module",
  "main": "./dist/cjs/index.js",
  "module": "./dist/esm/index.mjs",
  "types": "./dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/types/index.d.ts",
        "module": "./dist/esm/index.mjs"
      },
      "require": {
        "types": "./dist/types/index.d.ts",
        "main": "./dist/cjs/index.js"
      }
    },
    "./style.css": "./dist/style.css"
  },
  "publishConfig": {
    "registry": "https://registry.npmjs.org",
    "access": "public"
  },
  "packageManager": "yarn@4.1.1",
  "scripts": {
    "typecheck": "tsc --emitDeclarationOnly --declaration --declarationMap",
    "build:turbo": "run -T turbo build --filter='diagram'",
    "build": "vite build && run typecheck",
    "lint": "run -T eslint src/ --fix",
    "clean": "run -T rimraf dist bundle"
  },
  "dependencies": {
    "@likec4/core": "workspace:*",
    "@xyflow/react": "^12.0.0-next.12",
    "clsx": "^2.1.0",
    "framer-motion": "^11.0.25",
    "react-tracked": "^1.7.14"
  },
  "peerDependencies": {
    "@mantine/core": "^7.7.0",
    "@mantine/hooks": "^7.7.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@lume/kiwi": "^0.4.3",
    "@mantine/core": "^7.7.0",
    "@mantine/hooks": "^7.7.0",
    "@mantine/vanilla-extract": "^7.7.0",
    "@react-hookz/web": "^24.0.4",
    "@total-typescript/ts-reset": "^0.5.1",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vanilla-extract/css": "^1.14.2",
    "@vanilla-extract/dynamic": "^2.1.0",
    "@vanilla-extract/esbuild-plugin": "^2.3.5",
    "@vanilla-extract/recipes": "^0.5.2",
    "@vitejs/plugin-react": "^4.2.1",
    "execa": "^8.0.1",
    "fast-equals": "^5.0.1",
    "khroma": "^2.1.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "remeda": "^1.56.0",
    "terser": "^5.30.0",
    "tilg": "^0.1.1",
    "type-fest": "^4.13.1",
    "typescript": "^5.4.3",
    "unbuild": "^2.0.0",
    "vite": "^5.2.6",
    "vite-plugin-dts": "^3.8.1"
  }
}
