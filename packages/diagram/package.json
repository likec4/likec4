    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-tracked": "^1.7.11",
    "remeda": "^1.56.0",
    "scheduler": "^0.23.0",
    "terser": "^5.30.0",
    "tilg": "^0.1.1",
    "type-fest": "^4.13.1",
    "typescript": "^5.4.3",
    "unbuild": "^2.0.0",
