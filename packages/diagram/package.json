{
  "name": "@likec4/diagram",
  "version": "1.25.0",
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/diagram"
  },
  "files": [
    "dist",
    "src",
    "!**/*.spec.*",
    "!**/*.d.ts.map"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    },
    "./package.json": "./package.json",
    "./diagram.css": "./dist/diagram.css"
  },
  "packageManager": "pnpm@10.6.1",
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build": "vite build",
    "build:watch": "wireit",
    "lint": "run -T eslint src/ --fix",
    "clean": "rm -r -f -g \"dist/*\" .tsbuildinfo",
    "test": "vitest run --no-isolate"
  },
  "wireit": {
    "build:watch": {
      "command": "vite build --watch --mode development --clearScreen=false -l warn",
      "service": true
    }
  },
  "dependencies": {
    "@dagrejs/dagre": "1.1.4",
    "@likec4/core": "workspace:*",
    "@mantine/vanilla-extract": "catalog:",
    "@nanostores/react": "0.8.4",
    "@react-hookz/web": "^25.0.1",
    "@tabler/icons-react": "3.29.0",
    "@xstate/react": "^5.0.2",
    "@xyflow/react": "12.4.4",
    "@xyflow/system": "0.0.52",
    "bezier-js": "^6.1.4",
    "clsx": "^2.1.1",
    "d3-shape": "^3.2.0",
    "esm-env": "^1.2.2",
    "fast-equals": "^5.2.2",
    "framer-motion": "^12.4.10",
    "khroma": "^2.1.0",
    "nanostores": "0.11.4",
    "react-error-boundary": "^5.0.0",
    "remeda": "^2.21.0",
    "type-fest": "4.34.1",
    "xstate": "^5.19.2"
  },
  "peerDependencies": {
    "@mantine/core": "catalog:",
    "@mantine/hooks": "catalog:",
    "react": "~19.0.0",
    "react-dom": "~19.0.0"
  },
  "devDependencies": {
    "@likec4/tsconfig": "workspace:*",
    "@statelyai/inspect": "^0.4.0",
    "@types/bezier-js": "^4.1.3",
    "@types/d3-shape": "^3.1.7",
    "@types/react": "~19.0.10",
    "@types/react-dom": "~19.0.4",
    "@vanilla-extract/css": "^1.17.1",
    "@vanilla-extract/css-utils": "^0.1.4",
    "@vanilla-extract/vite-plugin": "^5.0.1",
    "@vitejs/plugin-react": "4.3.4",
    "typescript": "catalog:",
    "vite": "6.1.0",
    "vite-plugin-dts": "^4.5.0",
    "vitest": "^3.0.8",
    "wireit": "catalog:"
  }
}
