{
  "name": "@likec4/diagram",
  "version": "1.26.2",
  "license": "MIT",
  "bugs": "https://github.com/likec4/likec4/issues",
  "homepage": "https://likec4.dev",
  "author": "Denis Davydkov <denis@davydkov.com>",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/likec4/likec4.git",
    "directory": "packages/diagram"
  },
  "files": [
    "dist",
    "bundle",
    "src",
    "!**/*.spec.*",
    "!**/*.d.ts.map"
  ],
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "sources": "./src/index.ts",
      "default": {
        "types": "./dist/index.d.ts",
        "import": "./dist/index.js",
        "default": "./dist/index.js"
      }
    },
    "./bundle": {
      "sources": "./src/bundle/index.ts",
      "default": {
        "types": "./dist/bundle/index.d.ts",
        "import": "./bundle/index.js",
        "default": "./bundle/index.js"
      }
    },
    "./package.json": "./package.json"
  },
  "packageManager": "pnpm@10.6.5",
  "scripts": {
    "prebuild": "panda codegen",
    "typecheck": "tsc -b",
    "build:vite": "wireit",
    "build": "wireit",
    "bundle": "wireit",
    "lint": "run -T eslint src/ --fix",
    "clean": "rm -r -f -g \"dist/*\" .tsbuildinfo",
    "test": "vitest run --no-isolate"
  },
  "wireit": {
    "build:vite": {
      "command": "vite build"
    },
    "bundle": {
      "command": "vite build -c vite-bundle.config.ts"
    },
    "build": {
      "dependencies": [
        "build:vite",
        "bundle"
      ]
    }
  },
  "dependencies": {
    "@dagrejs/dagre": "1.1.4",
    "@likec4/core": "workspace:*",
    "@likec4/styles": "workspace:*",
    "@nanostores/react": "catalog:react",
    "@react-hookz/web": "catalog:utils",
    "@tabler/icons-react": "catalog:react",
    "@xstate/react": "^5.0.2",
    "@xyflow/react": "catalog:xyflow",
    "@xyflow/system": "catalog:xyflow",
    "bezier-js": "^6.1.4",
    "clsx": "catalog:react",
    "d3-shape": "^3.2.0",
    "esm-env": "catalog:utils",
    "fast-equals": "catalog:utils",
    "framer-motion": "catalog:react",
    "nanostores": "catalog:react",
    "react-error-boundary": "catalog:react",
    "react-shadow": "^20.6.0",
    "remeda": "catalog:utils",
    "type-fest": "catalog:utils",
    "xstate": "^5.19.2"
  },
  "peerDependencies": {
    "@mantine/core": "catalog:mantine",
    "@mantine/hooks": "catalog:mantine",
    "react": "catalog:react",
    "react-dom": "catalog:react",
    "@types/react": "catalog:react",
    "@types/react-dom": "catalog:react"
  },
  "devDependencies": {
    "@likec4/style-preset": "workspace:*",
    "@likec4/tsconfig": "workspace:*",
    "@pandabox/unplugin": "^0.2.1",
    "@pandacss/dev": "catalog:css",
    "@statelyai/inspect": "^0.4.0",
    "@types/bezier-js": "^4.1.3",
    "@types/d3-shape": "^3.1.7",
    "@vitejs/plugin-react": "catalog:vite",
    "postcss": "catalog:css",
    "typescript": "catalog:",
    "vite": "catalog:vite",
    "vite-plugin-dts": "catalog:vite",
    "vitest": "catalog:vitest",
    "wireit": "catalog:"
  }
}
