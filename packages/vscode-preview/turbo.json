{
  "$schema": "https://turbo.build/schema.json",
  "extends": [
    "//"
  ],
  "tasks": {
    "generate": {
      "dependsOn": [
        "@likec4/styles#generate"
      ],
      "outputs": []
    },
    "typecheck": {
      "dependsOn": [
        "@likec4/diagram#typecheck",
        "topo"
      ],
      "outputs": [
        "lib/"
      ],
      "outputLogs": "new-only"
    },
    "build": {
      "dependsOn": [
        "generate",
        "topo"
      ],
      "outputs": [
        "dist/**"
      ],
      "outputLogs": "new-only"
    }
  }
}
