{
  "name": "@likec4/vscode-preview",
  "private": true,
  "version": "0.37.1",
  "type": "module",
  "main": "src/index.ts",
  "module": "src/index.ts",
  "types": "src/index.ts",
  "exports": {
    ".": "./src/index.ts",
    "./protocol": "./protocol.ts"
  },
  "sideEffects": false,
  "files": [
    "dist",
    "!**/*.map"
  ],
  "scripts": {
    "typecheck": "tsc --noEmit",
    "build:watch": "vite build --watch --mode watch-dev",
    "build": "vite build --mode production",
