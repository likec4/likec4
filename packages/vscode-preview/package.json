    "@types/node": "^18.15.11",
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@types/vscode": "~1.77.0",
    "@types/vscode-webview": "^1.57.2",
    "@vitejs/plugin-react": "^4.1.0",
    "@vscode/webview-ui-toolkit": "^1.2.2",
    "lucide-react": "^0.279.0",
    "rambdax": "^9.1.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-error-boundary": "^4.0.11",
    "typescript": "^5.2.2",
    "vite": "^4.4.10",
    "vite-tsconfig-paths": "^4.2.1"
  }
