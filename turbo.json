{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV"
  ],
  "globalDependencies": [
    ".tool-versions",
    "package.json",
    "pnpm-workspace.yaml"
  ],
  "ui": "tui",
  "tasks": {
    "generate": {
      "outputs": [],
      "outputLogs": "new-only"
    },
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "generate",
        "topo"
      ],
      "outputs": [
        "*.tsbuildinfo",
        "lib/**"
      ],
      "outputLogs": "new-only"
    },
    "build": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "outputs": [
        "dist/**",
        "styled-system/**"
      ],
      "outputLogs": "new-only"
    }
  }
}
