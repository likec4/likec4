{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV"
  ],
  "globalDependencies": [
    ".tool-versions",
    "package.json",
    "pnpm-workspace.yaml"
  ],
  "ui": "tui",
  "tasks": {
    "generate": {
      "outputs": [],
      "outputLogs": "new-only"
    },
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^generate",
        "generate",
        "topo"
      ],
      "outputs": [],
      "outputLogs": "new-only"
    },
    "build": {
      "dependsOn": [
        "^generate",
        "generate",
        "topo"
      ],
      "outputs": [
        "dist/**"
      ],
      "outputLogs": "new-only"
    },
    "pack": {
      "dependsOn": [
        "build"
      ],
      "outputs": [
        "package.tgz"
      ],
      "outputLogs": "new-only"
    }
  }
}
