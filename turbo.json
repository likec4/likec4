{
  "$schema": "https://turbo.build/schema.json",
  "globalEnv": [
    "NODE_ENV"
  ],
  "globalDependencies": [
    ".tool-versions",
    "package.json"
  ],
  "ui": "tui",
  "tasks": {
    "generate": {
      "outputs": [],
      "outputLogs": "new-only"
    },
    "topo": {
      "dependsOn": [
        "^topo"
      ]
    },
    "typecheck": {
      "dependsOn": [
        "^generate",
        "topo",
        "generate"
      ],
      "outputs": [],
      "outputLogs": "new-only"
    },
    "build": {
      "dependsOn": [
        "^build",
        "generate"
      ],
      "outputs": [
        "dist/**"
      ],
      "outputLogs": "new-only"
    },
    "dev": {
      "dependsOn": [
        "^generate",
        "generate"
      ],
      "cache": false,
      "persistent": true
    },
    "clean": {
      "cache": false
    }
  }
}
