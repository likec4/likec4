{
  "name": "likec4-workspaces",
  "version": "0.20.3",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@3.5.1",
  "workspaces": [
    "docs",
    "packages/*",
    "examples/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "compile:all": "turbo run compile",
    "compile": "turbo run compile --filter='./packages/*'",
    "build": "turbo run build --filter='./packages/*'",
    "release": "shipjs prepare",
    "clean": "turbo run clean && rimraf -v -g '**/*.tsbuildinfo' '**/.turbo' 'node_modules/.cache/turbo'",
    "clean:after:publish": "rimraf -v -g '**/package*.tgz'",
    "dev": "turbo run dev --filter='./packages/*'",
    "test": "vitest run",
    "test:watch": "vitest",
    "lint": "turbo run lint --continue --filter='./packages/*'",
    "vitest:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/eslint": "^8.40.1",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.9",
    "@typescript-eslint/parser": "^5.60.0",
    "@vitest/ui": "^0.32.2",
    "eslint": "^8.42.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "shipjs": "^0.26.3",
    "turbo": "^1.10.3",
    "typescript": "^5.1.3",
    "vite": "^4.3.9",
    "vitest": "^0.32.0",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "1.22.19"
  },
  "resolutions": {
    "vscode-jsonrpc": "8.1.0",
    "vscode-languageclient": "8.1.0",
    "vscode-languageserver": "8.1.0",
    "vscode-languageserver-protocol": "3.17.3"
  }
}
