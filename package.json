{
  "name": "root",
  "version": "0.3.1",
  "private": true,
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "packages/*",
    "examples/*"
  ],
  "engines": {
    "node": ">=17"
  },
  "scripts": {
    "compile": "turbo run compile",
    "build": "turbo run build",
    "release-build": "turbo run clean release:build",
    "clean": "turbo run clean",
    "clean:after:publish": "rimraf --glob '**/package*.tgz' --interactive",
    "dev": "turbo run dev",
    "test": "turbo run test",
    "test:watch": "turbo run test:watch",
    "lint": "turbo run lint --continue",
    "vitest:ui": "vitest --ui --config vitest.config.ts"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.15.11",
    "@typescript-eslint/eslint-plugin": "^5.57.1",
    "@typescript-eslint/parser": "^5.57.1",
    "@vitest/ui": "^0.29.8",
    "eslint": "^8.37.0",
    "rimraf": "^4.4.1",
    "turbo": "^1.8.8",
    "typescript": "^5.0.3",
    "vite": "^4.2.1",
    "vitest": "^0.29.8"
  },
  "volta": {
    "node": "18.15.0"
  }
}
