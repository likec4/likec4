{
  "name": "likec4-workspaces",
  "version": "0.7.0",
  "private": true,
  "license": "MIT",
  "packageManager": "yarn@3.4.1",
  "workspaces": [
    "docs",
    "packages/*",
    "examples/*"
  ],
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "compile": "turbo run compile --filter='./packages/*'",
    "build": "turbo run build --filter='./packages/*'",
    "release": "run clean && turbo run build compile --filter='./packages/*'",
    "clean": "turbo run clean && rimraf -v -g '**/*.tsbuildinfo' '**/.turbo' 'node_modules/.cache/turbo'",
    "clean:after:publish": "rimraf -v -g '**/package*.tgz'",
    "dev": "turbo run dev --filter='./packages/*'",
    "test": "turbo run test --filter='./packages/*'",
    "test:watch": "turbo run test:watch --filter='./packages/*'",
    "lint": "turbo run lint --continue --filter='./packages/*'",
    "vitest:ui": "vitest --ui"
  },
  "devDependencies": {
    "@types/eslint": "^8.37.0",
    "@types/node": "^18.15.11",
    "@types/prettier": "^2.7.2",
    "@typescript-eslint/eslint-plugin": "^5.59.1",
    "@typescript-eslint/parser": "^5.59.1",
    "@vitest/ui": "^0.31.0",
    "eslint": "^8.39.0",
    "eslint-config-prettier": "^8.8.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.0",
    "turbo": "^1.9.3",
    "typescript": "^5.0.4",
    "vite": "^4.3.3",
    "vitest": "^0.30.1",
    "vitest-github-actions-reporter": "^0.10.0"
  },
  "volta": {
    "node": "18.16.0",
    "yarn": "2.4.3"
  },
  "resolutions": {
    "vscode-jsonrpc": "8.1.0",
    "vscode-languageclient": "8.1.0",
    "vscode-languageserver": "8.1.0",
    "vscode-languageserver-protocol": "3.17.3"
  }
}
